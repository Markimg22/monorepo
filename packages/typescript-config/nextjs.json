{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2017",
        "lib": [
            "ES2017",
            "DOM",
            "DOM.Iterable"
        ],
        "jsx": "preserve",
        "allowJs": true,
        "incremental": true,
        "noEmit": true
    }
}
